
<article class="noteList col-md-12">
	<ul class="noteBox">
	<% PostGet_all.forEach(	function(PostGet_all){%>
		<li class="noteLink container-fluid">
			<div class="noteBoxTitle col-md-8">
				<h3 class="container-fluid">
					<p  title="<%= PostGet_all.kt_isNote_title %>" target="_blank"><%= PostGet_all.kt_isNote_title %></p>
				</h3>
			</div>
			<div class="noteBoxDate col-md-4">
				<span class="container-fluid">
				<%= PostGet_all.kt_isTag_name %> |
					<%= Date(PostGet_all.kt_isNote_date) %>
				</span>
			</div>
			<div class="noteBoxbody col-md-12">
				<p class="container-fluid"><%- PostGet_all.kt_isNote_content %>
				</p>
			</div>
		</li>
	<% });%>
	</ul>
	<div id="pagination">
		<%- Create_links %>
	</div>
	<div style="clear:both;"></div>
</article>