								<td  class="deleteCatalogueTh10"><%= PostCatalogue.kt_navigation_ids %></td>
								<td  class="deleteCatalogueTh20"><%= PostCatalogue.kt_navigation_name %></td>
								<td  class="deleteCatalogueTh50"><a target="_blank" href="<%= PostCatalogue.kt_navigation_url %>"><%= PostCatalogue.kt_navigation_url %></a></td>
